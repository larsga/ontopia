<tolog:context topicmap="i18n.ltm">
  Show that a variable can be in the "ontopia" and "oks" scopes.
    <tolog:set var="var1" scope="ontopia">some body content1</tolog:set>
    <tolog:out var="var1"/>
    <tolog:set var="var2" scope="oks">some body content2</tolog:set>
    <tolog:out var="var2"/>
</tolog:context>
