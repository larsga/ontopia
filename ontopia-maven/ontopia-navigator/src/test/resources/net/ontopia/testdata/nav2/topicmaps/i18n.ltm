/* ===== LANGUAGES AND SCRIPTS ===== */

[natural-scripts : script-category = "Natural scripts"]
{natural-scripts, definition, [[Natural scripts are scripts which were
created in order to serve a living natural language, and used by ordinary
people to serve their day-to-day needs.]]}

/* --- Script groups and families --- */

[semitic       : script-group  = "Semitic scripts"]
[greek-f       : script-family = "Greek script family"]
[south-semitic : script-family = "Southern semitic script family"]
[north-semitic : script-family = "Northern semitic script family"]
[aramaic-f     : script-family = "Aramaic script family"]
belongs-to([natural-scripts] : container, [semitic] : containee)

{semitic, definition, [[Contains the scripts descended from the
proto-caananite script (also known as the northern linear abjad).
These scripts show great systematic variation, and are united mainly
by their common descent. The brahmic scripts have been separated out
into a group of their own.]]}

{north-semitic, definition, [[The ancestral northern linear abjad
quickly developed distinct northern and southern variants. The
scripts in this group are the descendants of the northern variant.]]}

{south-semitic, definition, [[The ancestral northern linear abjad
quickly developed distinct northern and southern variants. The
scripts in this group are the descendants of the southern variant.]]}

{greek-f, definition, [[Contains the greek alphabet and all its
descendants, which are also alphabets. The scripts in this family
all follow much the same principles.]]}

{aramaic-f, definition, [[Contains the scripts derived from the
aramaic abjad. These scripts have great family similarities, although
some have developed into full alphabets. The brahmic scripts descend
from the aramaic abjad, but have their own script group.]]}

belongs-to([semitic] : container, [greek-f]  : containee)
belongs-to([semitic] : container, [south-semitic] : containee)
belongs-to([semitic] : container, [north-semitic] : containee)
belongs-to([semitic] : container, [aramaic-f] : containee)


[kharoshthi : alphasyllabary = "Kharoshthi"
 @"http://psi.ontopia.net/iso/15924.xtm#khar"]
{kharoshthi, period-of-use, [[500 B.C.E. - 300 C.E.]]}
{kharoshthi, writing-direction, [[Right to left.]]}
{kharoshthi, article-about, "http://www.omniglot.com/writing/kharosthi.htm"}
{kharoshthi, article-about, "http://www.ancientscripts.com/kharosthi.html"}
derived-from([aramaic-s] : predecessor, [kharoshthi] : successor)
belongs-to([north-indic] : container, [kharoshthi] : containee)

/* http://www.engr.mun.ca/~adluri/telugu/language/script/chart.jpg */
[brahmic     : script-group   = "Brahmic scripts"]
belongs-to([natural-scripts] : container, [brahmic] : containee)
[brahmic-s   : alphasyllabary = "Brahmi"
 @"http://psi.ontopia.net/iso/15924.xtm#brah"]
{brahmic-s, period-of-use, [[500 B.C.E. - 600? C.E.]]}
{brahmic-s, writing-direction, [[Left to right.]]}
{brahmic-s, article-about, "http://www.ancientscripts.com/brahmi.html"}
{brahmic-s, article-about, "http://www.omniglot.com/writing/brahmi.htm"}
{brahmic-s, article-about, "http://www.ucl.ac.uk/~ucgadkw/position/salomon.html"}
{brahmic, article-about, "http://www.geocities.com/Athens/Academy/9594/brahmi.html"}
{brahmic, site-about, "http://www.cs.colostate.edu/~malaiya/scripts.html"}
{brahmic, article-about, "http://ourworld.compuserve.com/homepages/stone_catend/tr1.htm"}
{brahmic, article-about, "http://www.indiaheritage.com/science/ling.htm"}

[north-indic : script-family = "North Indic scripts"]
[south-indic : script-family = "South Indic scripts"]
[se-asian    : script-family = "South-East Asian scripts"]
[insular     : script-family = "Insular scripts of South-East Asia"]

belongs-to([brahmic]     : container, [north-indic] : containee)
belongs-to([brahmic]     : container, [south-indic] : containee)
belongs-to([brahmic]     : container, [se-asian]    : containee)
belongs-to([brahmic]     : container, [insular]     : containee)
belongs-to([north-indic] : container, [brahmic-s]   : containee)

{brahmic, definition, [[Contains all the scripts derived from Ashokan
brahmi used throughout southern Asia. The scrips are all of the same
basic type, though with considerable variations.]]}

{north-indic, definition, [[The north indic scripts are the scripts
derived from the northern branch of Ashokan brahmi. These scripts
show clear family resemblances.]]}

{south-indic, definition, [[The south indic scripts are the scripts
derived from the southern branch of Ashokan brahmi. These scripts
show clear family resemblances.]]}

{se-asian, definition, [[Contains the scripts derived from the Pallava
script that were used throughout the mainland of south-east Asia.]]}

{insular, definition, [[Contains the scripts derived from the Pallava
script that were used throughout the islands of south-east Asia.]]}

[sinitic : script-group = "Sinitic scripts"]
belongs-to([natural-scripts] : container, [sinitic] : containee)

{sinitic, definition, [[The sinitic scripts are the scripts derived
from the Chinese scripts, as well as a number of other scripts
designed to resemble it graphically, or to be used together with it
for various purposes.]]}


