/* ===== ONTOLOGY ===== */

[script = "Script"
  @"http://psi.ontopia.net/i18n/#script"]
{script, site-about ,"http://www.ancientscripts.com/"}
{script, site-about ,"http://www.omniglot.com/writing/"}
{script, article-about, "http://www.krysstal.com/writing.html"}
{script, article-about, "http://gurpsnet.sjgames.com/Archive/AdsDisSkills/Languages/languages.writing"}
{script, article-about, "http://www.egt.ie/standards/iso15924/document/n02.html"}
{script, definition, [[The general term for a writing system
consisting of a signary and the rules for assembling signs from
the signary into text.]]}

[alphabet      = "Alphabet"]   
subclass-of([alphabet]      : subclass, [script] : superclass)
{alphabet, definition, [[A type of writing system that denotes consonants
and vowels with separate characters.]]}

[abjad         = "Abjad"]
subclass-of([abjad]         : subclass, [script] : superclass)
{abjad, definition, [[A type of writing system where only consonants
are generally written. Some abjads have signs for vowels, but use
them only in special cases, such as loanwords or special kinds of
texts.]]}

[abugida       = "Abugida"]
subclass-of([abugida]       : subclass, [script] : superclass)
{abugida, definition, [[A type of writing system whose basic
characters denote consonants followed by a particular vowel,
and in which diacritics denote the other vowels.]]}

[alphasyllabary = "Alphasyllabary"]
subclass-of([alphasyllabary] : subclass, [script] : superclass)
{alphasyllabary, definition, [[A type of writing system whose
vowels are denoted by subsidiary symbols, not all of which
occur in a linear order (with relation to the consonant symbols)
that is congruent with their temporal order in speech.]]}

[logosyllabary = "Logosyllabary"]
subclass-of([logosyllabary] : subclass, [script] : superclass)
{logosyllabary, definition, [[A type of writing system whose
characters denote morphemes, and a subset of whose characters
can be used for their phonetic syllabic values without regard
to their semantic values.]]}

[syllabary     = "Syllabary"]
subclass-of([syllabary]     : subclass, [script] : superclass)
{syllabary, definition, [[A type of writing system whose
characters denote syllables, with no deliberate graphic similarity
between characters denoting phonetically similar syllables.]]}

[featural      = "Featural script"]
subclass-of([featural]      : subclass, [script] : superclass)
{featural, definition, [[A type of writing system whose
characters denote phonetic features.]]}

