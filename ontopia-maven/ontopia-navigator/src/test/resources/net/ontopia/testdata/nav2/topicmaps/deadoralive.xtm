    <variant>
      <parameters><topicRef xlink:href="#tagname"/></parameters>
      <variantName><resourceData>America's answer to all these Britney clones.</resourceData></variantName>
    </variant>
  </baseName>
</topic>

<!-- reify an occurrence -->
<topic>
  <subjectIdentity><subjectIndicatorRef xlink:href="#occ2"/></subjectIdentity>
  <baseName>
    <baseNameString>Elvis's death</baseNameString>
  </baseName>
  <occurrence>
    <instanceOf>
      <topicRef xlink:href="#description"/>
    </instanceOf>
    <resourceData>Know one knows for sure if Elvis is dead. His memory lives on.</resourceData>
  </occurrence>
</topic>
<topic>
  <subjectIdentity><subjectIndicatorRef xlink:href="#occ1"/></subjectIdentity>
  <baseName>
    <baseNameString>Elvis has an Unofficial Website</baseNameString>
  </baseName>
  <occurrence>
    <instanceOf>
      <topicRef xlink:href="#description"/>
    </instanceOf>
    <resourceData>Elvis had many fans during his performing years. This adulation has been continued on the internet.</resourceData>
  </occurrence>
</topic>

<topic id="t.influence">
  <subjectIdentity><subjectIndicatorRef xlink:href="#influence"/></subjectIdentity>
  <baseName>
    <baseNameString>Blues influence on Elvis</baseNameString>
  </baseName>
  <occurrence>
    <instanceOf>
      <topicRef xlink:href="#description"/>
    </instanceOf>
    <resourceData>Elvis was heavily influenced by blues music. This changed the course of rock and pop music.</resourceData>
  </occurrence>
</topic>

<!-- reify baseName -->
<topic id="t.theKingName">
  <subjectIdentity><subjectIndicatorRef xlink:href="#theKingName"/></subjectIdentity>
  <baseName>
    <baseNameString>"The King" name</baseNameString>
    <variant>
      <parameters><topicRef xlink:href="#description"/></parameters>
      <variantName><resourceData>'The King' as a name has been popularised. Those labelled with this name are ranked alongside the legendary Elvis.</resourceData></variantName>
    </variant>
  </baseName>
</topic>

<!-- reify variantName -->
<topic id="t.kingName">
  <subjectIdentity><subjectIndicatorRef xlink:href="#kingName"/></subjectIdentity>
  <baseName>
    <baseNameString>"King" sort name</baseNameString>
    <variant>
      <parameters><topicRef xlink:href="#description"/></parameters>
      <variantName><resourceData>'King' is used as a sort name for 'The King' so that it appears under 'K' in alphabetical listings of topics.</resourceData></variantName>
    </variant>
  </baseName>
</topic>
