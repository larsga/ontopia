    </variant>
  </baseName>
</topic>


<!-- reify another resource in an occurrence -->
<topic id="americansforcloningelvis">
  <instanceOf><topicRef xlink:href="#website"/></instanceOf>
  <subjectIdentity><resourceRef xlink:href="http://www.geocities.com/Vienna/1673/"/></subjectIdentity>
